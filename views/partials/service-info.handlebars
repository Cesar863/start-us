<article class="service">
    <div class="title">
        {{service_title}}
        {{service_type}} 
        <span>- (Created on
        {{format_date created_at}} by {{user.username}} 
        </span>
        
    </div>
    <div class="meta">
        <h5>Description:</h5>
        {{service_description}}
        <h5>Budget:</h5>
        {{budget}}
        
    </div>
</article>